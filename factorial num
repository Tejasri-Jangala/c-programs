#include<stdio.h>
#include<conio.h>
int factorial(int);
int main()
{
	int i,num,fact;
	printf("enter the positive integer");
	scanf("%d",&num);
	fact=factorial(num);
	printf("factorial of %d is %d",num,fact);
	return 0;
}
int factorial(int n)
{
	if(n==0)
	return(1);
	else
	return(n*factorial(n-1));
	
	
}
